<h1>New Person</h1>

<%= semantic_form_for @person do |f| %>
  <%= f.inputs do %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :phone_number %>
    <%= f.input :city %>
    <%= f.input :email %>
    <%= f.input :password %>
  <% end %>
  <%= f.actions %>
<% end %>

<%= link_to 'Back', root_path %>
